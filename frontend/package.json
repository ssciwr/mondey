{
	"name": "mondey-frontend-prototype",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest --coverage",
		"test:ui": "pnpm exec playwright test tests/ui",
		"test:ui:dev": "pnpm exec playwright test tests/ui --ui",
		"test:e2e": "pnpm exec playwright test tests/e2e",
		"test:e2e:dev": "pnpm exec playwright test tests/e2e --ui",
		"test:e2e:dev:seq": "pnpm exec playwright test tests/e2e --ui --workers 1",
		"openapi-ts": "openapi-ts"
	},
	"devDependencies": {
		"@hey-api/openapi-ts": "0.55.2",
		"@playwright/test": "^1.50.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.18.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/svelte": "^5.2.7",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.13.9",
		"@vitest/coverage-v8": "^2.1.9",
		"autoprefixer": "^10.4.20",
		"flowbite": "^2.5.2",
		"flowbite-svelte": "^0.47.4",
		"flowbite-svelte-icons": "2.0.2",
		"globals": "^15.15.0",
		"jsdom": "^25.0.1",
		"svelte": "^5.22.5",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0",
		"vite": "^5.4.14",
		"vitest": "^2.1.9"
	},
	"type": "module",
	"dependencies": {
		"@hey-api/client-fetch": "0.4.3",
		"@unovis/ts": "1.5.0-beta.0",
		"cdigit": "^4.0.2",
		"danfojs": "^1.1.2",
		"export-to-csv": "^1.4.0",
		"iso-639-1": "3.1.3",
		"svelte-dnd-action": "^0.9.57"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@sveltejs/kit",
			"core-js",
			"esbuild",
			"maplibre-gl"
		]
	}
}
